{
  "buildCommand": "prisma generate && remix vite:build",
  "installCommand": "npm install",
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "env": {
    "PRISMA_CLI_QUERY_ENGINE_TYPE": "binary",
    "PRISMA_CLIENT_ENGINE_TYPE": "binary"
  },
  "alias": ["rainbows-dreams-wholesale-app.vercel.app"]
}
